   <text>
      <body>
         <pb n="charta 14 recto" facs="../images/vat-lat3195-f/vat-lat3195-f-014r.jpg" />
         <lg xml:id="rvf062" type="sonnet" n="62">
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><hi rendition="#red #fs24pt">P</hi><hi rendition="#small-caps">a</hi>dre del ciel<supplied>,</supplied> dopo i perduti giorni<supplied>,</supplied></l>
                     <l n="2">Dopo le notti <choice><orig>uaneggiando</orig><reg>vaneggiando</reg></choice> spese&v2c;</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3">Con quel fero desio<choice><orig> &virg;</orig><reg/></choice> <choice><orig>chal</orig><reg>ch’al</reg></choice> cor <choice><orig>saccese</orig><reg>s’accese</reg></choice>&p2c;</l>
                     <l n="4">Mirando <choice><orig>gliatti</orig><reg>gli atti</reg></choice> per mio mal &si; adorni&p2c;</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5">Piacciati omai<supplied>,</supplied> col tuo lume<supplied>,</supplied> <choice><orig>chio</orig><reg>ch’io</reg></choice> torni</l>
                     <l n="6">Ad altra <choice><orig>uita</orig><reg>vita</reg></choice>&v2c; et a &piu; belle imprese&p2c;</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7">&Si; <choice><orig>chauendo</orig><reg>ch’avendo</reg></choice> le reti indarno tese&v2c;</l>
                     <l n="8">Il mio duro <choice><orig>aduersario</orig><reg>adversario</reg></choice> se ne scorni.</l>
                 </lg>
             </lg>
           <lg type="sestet">
            <lg type="dblvrs" corresp="#canvasline">
                <l n="9">Or <choice><orig>uolge</orig><reg>volge</reg></choice><supplied>,</supplied> <choice><orig>signor</orig><reg>Signor</reg></choice> mio <choice><orig>lundecimo</orig><reg>l’undecimo</reg></choice> anno&p2c;</l>
                <l n="10"><choice><orig>Chi</orig><reg>Ch’i</reg></choice><supplied>’</supplied> fui <choice><abbr>sōmesso</abbr><expan>sommesso</expan></choice> al dispietato giogo<supplied>,</supplied></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="11">Che sopra i &piu; soggetti &e; &piu; feroce.</l>
                <l n="12">Miserere del mio non degno affanno&p2semicolon;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="13">Reduci i pensier<supplied>’</supplied> <choice><orig>uaghi</orig><reg>vaghi</reg></choice> a miglior luogo&p2semicolon;</l>
                <l n="14">Ramenta lor come oggi fusti in croce.</l>
            </lg>
           </lg>
         </lg>
         <space corresp="#canvasline"/>
         <lg xml:id="rvf063" type="ballata_grande" n="63">
            <lg type="dblvrs" corresp="#canvasline">
                <l n="1"><hi rendition="#blue #fs24pt">V</hi><hi rendition="#small-caps">o</hi>lgendo gli occhi al mio <choice><orig>nouo</orig><reg>novo</reg></choice> colore</l>
                <l n="2">Che fa di morte rimembrar la gente<supplied>,</supplied></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="3"><choice><orig>Pieta</orig><reg>Pietà</reg></choice> <choice><orig>ui</orig><reg>vi</reg></choice> mosse<choice><orig> .</orig><reg>,</reg></choice> onde<supplied>,</supplied> benignamente<choice><orig> &virg;</orig><reg/></choice></l>
                <l n="4">Salutando<supplied>,</supplied> teneste in <choice><orig>uita</orig><reg>vita</reg></choice> il core.</l>
            </lg>
            <lg class="indent" type="dblvrs" corresp="#canvasline">
                <l n="5"><hi rendition="#red #fs24pt">&pilcrow2section;</hi>La fraile <choice><orig>uita</orig><reg>vita</reg></choice><supplied>,</supplied> <choice><orig>chanchor</orig><reg>ch’anchor</reg></choice> meco alberga<supplied>,</supplied></l>
                <l n="6">Fu de<supplied>’</supplied> <choice><orig>be gliocchi</orig><reg>begli occhi</reg></choice> <choice><orig>uostri</orig><reg>vostri</reg></choice> aperto dono&p2c;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="7">Et de la <choice><orig>uoce</orig><reg>voce</reg></choice> angelica <choice><orig>soaue</orig><reg>soave</reg></choice>&p2c;</l>
                <l n="8">Da lor conosco <choice><orig>lesser</orig><reg>l’esser</reg></choice> <choice><orig>ouio</orig><reg>ov’io</reg></choice> sono&p2semicolon;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="9"><choice><orig>Che</orig><reg>Ché</reg></choice><supplied>,</supplied> come suol pigro animal &per; <choice><orig>uerga</orig><reg>verga</reg></choice>&p2c;</l>
                <l n="10"><choice><orig>Cosi</orig><reg>Così</reg></choice> destaro in me <choice><orig>lanima¦graue</orig><reg>l’anima grave</reg></choice>.</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="11">Del mio cor<supplied>,</supplied> donna<supplied>,</supplied> <choice><orig>luna</orig><reg>l’una</reg></choice> &amp7; <choice><orig>laltra</orig><reg>l’altra</reg></choice> <choice><orig>chiaue</orig><reg>chiave</reg></choice></l>
                <l n="12"><choice><orig>Auete</orig><reg>avete</reg></choice> in mano<choice><orig> .</orig><reg>;</reg></choice> &amp7; di <choice><orig>cio</orig><reg>ciò</reg></choice> son contento&p2c;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="13">Presto di <choice><orig>nauigare</orig><reg>navigare</reg></choice> a ciascun <choice><orig>uento</orig><reg>vento</reg></choice>&p2c;</l>
                <l n="14"><choice><orig>Chogni</orig><reg>Ch’ogni</reg></choice> cosa da <choice><orig>uoi</orig><reg>voi</reg></choice> <choice><orig>me</orig><reg>m’è</reg></choice> dolce honore.</l>
            </lg>
         </lg>
         <space corresp="#canvasline"/>
         <lg xml:id="rvf064" type="sonnet" n="64">
          <lg type="octave">
            <lg type="dblvrs" corresp="#canvasline">
                <l n="1"><hi rendition="#blue #fs24pt">S</hi><hi rendition="#small-caps">e</hi> <choice><orig>uoi</orig><reg>voi</reg></choice> poteste per turbati segni&v2c;</l>
                <l n="2">Per chinar gli occhi&v2c; o per pieghar la testa&p2c;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="3">o &per; esser &piu; <choice><orig>daltra</orig><reg>d’altra</reg></choice> al fuggir presta&v2c;</l>
                <l n="4"><choice><orig>Torcendol</orig><reg>Torcendo ’l</reg></choice> <choice><orig>uiso</orig><reg>viso</reg></choice> a<supplied>’</supplied> preghi honesti &amp7; degni&p2c;</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="5"><choice><orig>Vscir</orig><reg>Uscir</reg></choice> giamai <choice><orig>ouer</orig><reg>over</reg></choice> &per; altri ingegni<supplied>,</supplied></l>
                <l n="6">Del petto <choice><orig>oue</orig><reg>ove</reg></choice> dal primo lauro <choice><abbr>īnesta</abbr><expan>innesta</expan></choice></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="7">Amor &piu; rami<choice><orig> .</orig><reg>,</reg></choice> i<supplied>’</supplied> direi ben che <choice><abbr>&q-tilde;sta</abbr><expan>questa</expan></choice></l>
                <l n="8">Fosse giusta cagione a<supplied>’</supplied> <choice><orig>uostri</orig><reg>vostri</reg></choice> sdegni&p2semicolon;</l>
            </lg>
          </lg>
            <lg type="sestet">
            <lg type="dblvrs" corresp="#canvasline">
                <l n="9"><choice><orig>Che</orig><reg>Ché</reg></choice> gentil pianta in arido terreno</l>
                <l n="10">Par che si <choice><orig>disconuenga</orig><reg>disconvenga</reg></choice><choice><orig> .</orig><reg>,</reg></choice> et <choice><orig>pero</orig><reg>però</reg></choice> lieta</l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="11">Naturalmente quindi si diparte.</l>
                <l n="12">Ma poi <choice><orig>uostro</orig><reg>vostro</reg></choice> destino a <choice><orig>uoi</orig><reg>voi</reg></choice> pur <choice><orig>uieta</orig><reg>vieta</reg></choice></l>
            </lg>
            <lg type="dblvrs" corresp="#canvasline">
                <l n="13"><choice><orig>Lesser</orig><reg>L’esser</reg></choice> <choice><orig>altroue</orig><reg>altrova</reg></choice>&p2c; <choice><orig>prouedete</orig><reg>provedete</reg></choice> almeno</l>
                <l n="14">Di non star sempre in odiosa parte.</l>
            </lg>
            </lg>
         </lg>
         <space corresp="#canvasline"/>
         <lg xml:id="rvf065" type="sonnet" n="65">
             <lg type="octave">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="1"><hi rendition="#red #fs24pt">L</hi><hi rendition="#small-caps">a</hi>sso<supplied>,</supplied> che mal accorto fui da prima</l>
                     <l n="2">Nel giorno <choice><orig>cha</orig><reg>ch’a</reg></choice> ferir mi <choice><orig>uenne</orig><reg>venne</reg></choice> <choice><orig>amore</orig><reg>Amore</reg></choice>&p2c;</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="3"><choice><orig>Cha</orig><reg>Ch’a</reg></choice> passo a passo &e; po<subst><del type="erasure"><gap reason="illegible"/></del><add>i</add></subst> fatto signore</l>
                     <l n="4">De la mia <choice><orig>uita</orig><reg>vita</reg></choice><choice><orig> .</orig><reg>,</reg></choice> et posto in su la cima.</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="5">Io non credea &per; <choice><orig>força</orig><reg>forza</reg></choice> di sua lima</l>
                     <l n="6">Che punto di <choice><orig>fermeçça</orig><reg>fermezza</reg></choice> o di <choice><orig>ualore</orig><reg>valore</reg></choice></l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="7">Mancasse mai ne <choice><orig>lindurato</orig><reg>l’indurato</reg></choice> core&p2semicolon;</l>
                     <l n="8">Ma <choice><orig>cosi</orig><reg>così</reg></choice> <choice><orig>ua</orig><reg>va</reg></choice> chi <choice><orig>sopral</orig><reg>sopra ’l</reg></choice> <choice><orig>uer</orig><reg>ver</reg></choice> <choice><orig>sestima</orig><reg>s’estima</reg></choice>.</l>
                 </lg>
             </lg>
             <lg type="sestet">
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="9">Da ora <choice><orig>inançi</orig><reg>inanzi</reg></choice> ogni difesa &e; tarda&p2c;</l>
                     <l n="10">Altra che di <choice><orig>prouar</orig><reg>provar</reg></choice> <choice><orig>sassai</orig><reg>s’assai</reg></choice> o poco</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="11">Questi preghi mortali <choice><orig>amore</orig><reg>Amore</reg></choice> sguarda.</l>
                     <l n="12">&Non; prego <choice><orig>gia</orig><reg>già</reg></choice>&v2c; <choice><orig>ne</orig><reg>né</reg></choice> puote <choice><orig>auer</orig><reg>aver</reg></choice> &piu; loco</l>
                 </lg>
                 <lg type="dblvrs" corresp="#canvasline">
                     <l n="13">Che mesuratamente il mio cor arda&p2c;</l>
                     <l n="14">Ma che sua parte abbi costei del foco.</l>
                 </lg>
             </lg>
         </lg>
      </body>
   </text>
</TEI>
